{
  "version": 2,
  "buildCommand": "cd client && CI=false npm run build",
  "outputDirectory": "client/build",
  "installCommand": "cd client && npm install",
  "functions": {
    "client/**/*.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}